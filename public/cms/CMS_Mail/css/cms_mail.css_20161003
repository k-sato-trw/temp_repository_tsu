
body {
	font-family: "ヒラギノ角ゴ Pro W3", "Hiragino Kaku Gothic Pro", "メイリオ", Meiryo, Osaka, "ＭＳ Ｐゴシック", "MS PGothic", sans-serif;
	font-size: 14px;
	line-height: 150%;
	background-color:#EEEEEE;

}


#wrapper {width:100%;}


#header {
	width:100%;
	height:60px;
	line-height:60px;
	background-color:#333333;
	color:#FFFFFF;
	font-size:24px;
	text-align:center;
	}
	#headerNavi {
		width:1000px;
		height:60px;
		margin:0 auto;
		position:relative;
		}
		/*三国ロゴボタン*/
		#headerNavi h2 {
			width:125px;
			height:26px;
			background:url(../images/logo_mikuni.png) no-repeat;
			text-indent:-9999px;
			display:block;
			position:absolute;
			top:50%;
			right:0;
			margin-top:-13px;
			}

	


#contents{}

	/*ナビゲーション*/
	#sub{
		width:100%;
		height:50px;
		padding-top:20px;
		border-bottom:6px solid #99CC00;
		background-color:#FFFFFF;
		}
		#subNavi ul{
			width:606px;
			margin:0 auto;
			}
			#subNavi ul li,
			#subNavi ul li a {
				width:200px;
				height:50px;
				line-height:56px;
				margin:0 1px;
				float:left;
				text-align:center;
				background-color:#B2B2B2;
				color:#D9D9D9;
				font-size:22px;
				border-radius:10px 10px 0 0;
				-webkit-border-radius:10px 10px 0 0;
				-moz-border-radius:10px 10px 0 0;
				text-decoration:none;
				}
			#subNavi ul li a:hover {
				background-color:#999999;
				color:#EEEEEE;
				}
			#subNavi ul li.select {
				background-color:#99CC00;
				color:#FFFFFF;
				font-weight:bold;
				}
	
	/*メイン*/
	#main{
		width:1000px;
		margin:0 auto;
		padding-top:20px;
		}
		
		/*未入力、保存中*/.i01 {color:#01BFFF!important;}
		/*　 配信確定　 */.i02 {color:#FF5A78!important;}
		/*　 配信完了　 */.i03 {color:#B2B2B2!important;}

		
		
#footer{
	width:100%;
	background-color:#333333;
	padding:30px 0;
	text-align:center;
	}
	#footer ul li a{
		text-align:center;
		color:#FFFFFF;
		text-decoration:none;
		display:block;
		}
	#footer ul li a:hover {background-color:#FFFF00; color:#333333;}
	#footer .btn01 a{background-color:#00BFFF;}
	#footer .btn02 a{background-color:#FF5A78;}



/*フォーム*/
input:focus,
textarea:focus,
select:focus {
	-webkit-box-shadow: 0px 1px 4px rgba(0, 0, 0, 0.5);
	-moz-box-shadow: 0px 1px 4px rgba(0, 0, 0, 0.5);
	box-shadow: 0px 1px 4px rgba(0, 0, 0, 0.5);
	background-color: #eee;	
}
input::-ms-clear {
    display: none;
}

.textbox,
.textarea {
	padding:4px;
	font-size:14px;
	font-family:Arial, Helvetica, sans-serif;
	border:solid 3px #666666;
}
.checkbox {
	border:solid 3px #666666;
}






/*------------------------------------

　　　　　　1.日付選択

------------------------------------*/

#contents .page01 {}

	/*注釈アイコン*/
	.page01 ul#icon {
		float:right;
		}
		.page01 ul#icon li {
			float:left;
			font-size:16px;
			font-weight:bold;
			height:28px;
			line-height:28px;
			margin-left:20px;
			}
			.page01 ul#icon li span {
				text-indent:-9999px;
				width:28px;
				height:28px;
				margin-left:2px;
				display:inline-block;
				background-image:url(../images/icon_mail.png);
				background-repeat:no-repeat;
				}
			.page01 ul#icon li.i01 span {background-position:-8px -42px;}
			.page01 ul#icon li.i02 span {background-position:-49px -42px;}
			.page01 ul#icon li.i03 span {background-position:-91px -42px;}
			
	/*カレンダーヘッダー*/
	.page01 #cal_header {
		width:998px;
		height:70px;
		background-color:#666666;
		margin:5px auto 0;
		position:relative;
		}
		/*前月次月ボタン*/
		.page01 #cal_header .btn a {
			width:86px;
			height:66px;
			line-height:70px;
			text-align:center;
			color:#333333;
			text-align:center;
			font-size:22px;
			font-weight:bold;
			text-decoration:none;
			border:2px solid #666666;
			background-color:#999999;
			background-image:url(../images/btn_arrow.png);
			background-repeat:no-repeat;
			position:absolute;
			top:0;
			}
			.page01 #cal_header .btn a:hover {background-color:#CCCCCC;}
			.page01 #cal_header #back a {left:0; background-position:-22px 50%; padding-left:20px;}
			.page01 #cal_header #next a {right:0; background-position:72px 50%;padding-right:20px;}
			
		/*年月*/
		.page01 #cal_header #year,
		.page01 #cal_header #month {
			font-family:Arial, Helvetica, sans-serif;
			font-weight:bold;
			top:0px;
			position:absolute;
			display:block;
			height:70px;
			line-height:70px;	
			}
		/*年*/
		.page01 #cal_header #year{
			width:70px;
			left:390px;
			color:#999999;
			font-size:30px;
			}
		/*月*/
		.page01 #cal_header #month{
			left:464px;
			color:#FFFFFF;
			font-size:70px;
			text-align:center;
			width:80px;
			}

		/*当月へ戻る*/
		.page01 #cal_header #now a{
			color:#CCCCCC;
			border:2px solid #CCCCCC;
			font-size:13px;
			font-weight:bold;
			width:80px;
			height:20px;
			line-height:20px;
			text-align:center;
			text-decoration:none;
			position:absolute;
			left:550px;
			top:50%;
			margin-top:-10px;
			border-radius: 4px;
			-webkit-border-radius: 4px;
			-moz-border-radius: 4px;
			}
			.page01 #cal_header #now a:hover {
				background-color:#CCCCCC;
				color:#333333;
				}

			
			

	/*カレンダー*/
	.page01 #cal table{
		width:1000px;
		table-layout:fixed;
		border-left:1px solid #DDDDDD;
		}
		.page01 #cal th {
			background-color:#666666;
			color:#FFFFFF;
			font-size:18px;
			font-weight:bold;
			text-align:center;
			height:30px;
			vertical-align:middle;
			border-top:1px solid #DDDDDD;
			border-bottom:1px solid #DDDDDD;
			border-right:1px solid #DDDDDD;
			}
		.page01 #cal td {
			height:100px;
			border-bottom:1px solid #DDDDDD;
			/*border-right:1px solid #DDDDDD;*/
			background:url(../images/td_border.png) top right repeat-y #FFFFFF;
			color:#666666;
			position:relative;
			
			}

			/*日付*/
			.page01 #cal td span.date{
				height:90px;
				padding:10px 0 0 10px;
				display:block;
				font-size:20px;
				font-weight:bold;
				font-family:Arial, Helvetica, sans-serif;
				position: relative;
				z-index:300;
				}
			/*日付:マウスオーバー*/
			.page01 #cal td span.date:hover {
				background-color:#ADD633;
				cursor:pointer;
				filter: alpha(opacity=40);
				-moz-opacity:0.40;
				opacity:0.40;
				text-decoration:none;
				}
				.page01 #cal td.select_no span.date:hover {
				background:none;
				cursor: default;
				text-decoration:none;
				filter: alpha(opacity=100);
				-moz-opacity:1;
				opacity:1;
				}
				
			/*レース名*/
			.page01 #cal td div {
				height:28px;
				color:#FFFFFF;
				line-height:28px;
				position:absolute;
				overflow:hidden;
				left:4px;
				bottom:4px;
				font-size:14px;
				text-align:center;
				border-radius: 4px;
				-webkit-border-radius: 4px;
				-moz-border-radius: 4px;
				z-index:200;
				}
				/*レース名：別月*/
				.page01 #cal td.day_no { background:url(../images/td_border2.png) top right repeat-y #EEEEEE;}
				.page01 #cal td.day_no span {
					background-color:#EEEEEE;
					filter: alpha(opacity=50);
					-moz-opacity:0.50;
					opacity:0.50;
					}
				/*レース幅*/
				.page01 #cal td div.sch01{ width:134px;}
				.page01 #cal td div.sch02{ width:276px;}
				.page01 #cal td div.sch03{ width:418px;}
				.page01 #cal td div.sch04{ width:564px;}
				.page01 #cal td div.sch05{ width:704px;}
				.page01 #cal td div.sch06{ width:848px;}
				.page01 #cal td div.sch07{ width:990px;}
				/*レース色*/
				.page01 #cal td div.g0{ background:#60AFFF;}
				.page01 #cal td div.g1{ background:#FF6600;}
				.page01 #cal td div.g2{ background:#FFB400;}
				.page01 #cal td div.g3{ background:#99CC00;}
				.page01 #cal td div.sg{ background:#CC0000;}
				.page01 #cal td div.chushi{background-color: #1E1E1E;color: #C70000;}
				/*土日カラー*/
				.page01 #cal th.sun,
				.page01 #cal td.sun {color:#FF5A78;}
				.page01 #cal th.sat {color:#7EDEFE;}
				.page01 #cal td.sat {color:#01BFFF;}
				
				/*メールアイコン*/
				.page01 #cal td div.i_mail,
				.page01 #cal td div.i_mail2 {
					width:50px;
					height:60px;
					line-height:normal;
					top:50%;
					left:50%;
					margin:-30px 0 0 -25px;
					font-family:Arial, Helvetica, sans-serif;
					font-weight:bold;
					font-size:16px;
					z-index:400;
					}
					.page01 #cal td div.i_mail a,
					.page01 #cal td div.i_mail2 a {text-decoration:none;}
					.page01 #cal td div.i01 a{color:#01BFFF;}
					.page01 #cal td div.i02 a{color:#FF5A78;}
					.page01 #cal td div.i03 a{color:#B2B2B2;}
					
				/*前検以外*/
				.page01 #cal td div.i_mail2 { top:36%;}
								
				/*1日2通*/
				.page01 #cal td div.i_mail.d1 { margin:-30px 0 0 -50px;}
				.page01 #cal td div.i_mail.d2 { margin:-30px 0 0 0;}
				.page01 #cal td div.i_mail2.d1 { margin:-30px 0 0 -40px;}
				.page01 #cal td div.i_mail2.d2 { margin:-30px 0 0 10px;}
				
				/*アイコン指定*/
				.page01 #cal td div.i_mail span,
				.page01 #cal td div.i_mail2 span {
						width:42px;
						height:42px;
						margin:0 auto;
						text-indent:-9999px;
						display:block;
						background-image:url(../images/icon_mail.png);
						background-repeat:no-repeat;
						}
				.page01 #cal td div.i01 span { background-position:0 0;}
				.page01 #cal td div.i02 span { background-position:-42px 0;}
				.page01 #cal td div.i03 span { background-position:-84px 0;}

				
				/*リンク設定*/
				.page01 #cal td div.i01 a:hover { color:#0180A9;}
				.page01 #cal td div.i02 a:hover { color:#BA2753;}
				.page01 #cal td div.i03 a:hover { color:#666666;}
				.page01 #cal td div.i01 a:hover span { background-position:-126px 0;}
				.page01 #cal td div.i02 a:hover span { background-position:-168px 0;}
				.page01 #cal td div.i03 a:hover span { background-position:-210px 0;}

