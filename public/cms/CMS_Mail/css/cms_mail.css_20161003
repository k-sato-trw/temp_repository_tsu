							/*ラジオボタン*/
							@media (min-width: 1px) {
							input[type=radio] { display: none;}
							
							
							.radio {
							  box-sizing: border-box;
							  position: relative;
							  display: inline-block;
							  margin: 0 20px 8px 0;
							  padding: 5px 2px 5px 20px;
							  vertical-align: middle;
							  cursor: pointer;
							  color:#888;
							  }
							.radio:hover,
							input[type=radio]:checked + .radio  {color: #333333;}
							
							.radio:hover:after,
							input[type=radio]:checked + .radio:after { border-color: #333333;}
							
							.radio:after {
							  position: absolute;
							  top: 50%;
							  left: 0px;
							  display: block;
							  margin-top: -10px;
							  width: 14px;
							  height: 14px;
							  border: 2px solid #888;
							  border-radius: 8px;
							  background-color:#FFFFFF;
							  z-index:100;
							  content: '';
							  }
							.radio:before {
							  position: absolute;
							  top: 50%;
							  left: 5px;
							  display: block;
							  margin-top: -5px;
							  width: 8px;
							  height: 8px;
							  border-radius: 50%;
							  background-color: #333333;
							  content: '';
							  z-index:200;
							  opacity: 0;
							  }
							input[type=radio]:checked + .radio:before {opacity: 1;}
						}
						
